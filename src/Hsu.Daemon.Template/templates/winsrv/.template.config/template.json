        },
        {
          "choice": "net6.0",
          "description": "Target net6.0"
        },
        {
          "choice": "net7.0",
          "description": "Target net7.0"
        }
      ],
      "replaces": "FrameworkParameter",
      "defaultValue": "net6.0"
    }
